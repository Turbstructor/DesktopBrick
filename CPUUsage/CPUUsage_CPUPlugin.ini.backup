[msThrdCnt]
UpdateDivider = -1
DynamicVariables = 1
Measure = Calc
Formula = %NUMBER_OF_PROCESSORS%
IfCondition = msThrdCnt < 16
IfTrueAction = [!SetVariable "ColorGrp5" "255, 255, 255, 31"][!DisableMeasure msCPUUsageT12][!DisableMeasure msCPUUsageT13][!DisableMeasure msCPUUsageT14][!DisableMeasure msCPUUsageT15]
IfCondition2 = msThrdCnt < 12
IfTrueAction2 = [!SetVariable ColorGrp4 "255, 255, 255, 31"][!DisableMeasure msCPUUsageT8][!DisableMeasure msCPUUsageT9][!DisableMeasure msCPUUsageT10][!DisableMeasure msCPUUsageT11]
IfCondition3 = msThrdCnt < 8
IfTrueAction3 = [!SetVariable ColorGrp3 "255, 255, 255, 31"][!DisableMeasure msCPUUsageT6][!DisableMeasure msCPUUsageT7]
IfCondition4 = msThrdCnt < 6
IfTrueAction4 = [!SetVariable ColorGrp2 "255, 255, 255, 31"][!DisableMeasure msCPUUsageT4][!DisableMeasure msCPUUsageT5]
IfCondition5 = msThrdCnt < 4
IfTrueAction5 = [!SetVariable ColorGrp1 "255, 255, 255, 31"][!DisableMeasure msCPUUsageT2][!DisableMeasure msCPUUsageT3]